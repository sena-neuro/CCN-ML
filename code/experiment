#!/usr/bin/env bash

# for i in /data2/s*.mat

Exps=('E50w50s' 'E50w25s' 'E25w25s' 'E40w20s')

declare -A params

params["E50w50s"]="50 50"
params["E50w25s"]="50 25"
params["E25w25s"]="25 25"
params["E40w20s"]="40 20"
minTrial="84"
for i in "${Exps[@]}"; do
	echo Experiment $i is started
	in_path="/Users/huseyinelmas/Desktop/data/"
	sv_path='/Users/huseyinelmas/Desktop/CCN-ML/code/experiments/'
	mkdir -p  $sv_path
	python ccn_wo_prior_main.py ${params[$i]} "$minTrial" "$in_path"  "$sv_path"
	echo Done 
done


