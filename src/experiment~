#!/usr/bin/env bash

Exps=("E80w40s")
declare -A params

params["E100w100s"]="100 100"
params["E100w50s"]="100 50"
params["E100w25s"]="100 25"
params["E50w50s"]="50 50"
params["E50w25s"]="50 25"
params["E25w25s"]="25 25"
params["E40w20s"]="40 20"
params["E20w20s"]="20 20"
params["E80w40s"]="80 40"

minTrial='0'
for i in "${Exps[@]}"; do
    echo Experiment $i is started
    in_path="/home/user/Desktop/EEG_data/Static/"
    sv_path="/home/user/Desktop/EEG_results/beta/$i/"
    mkdir -p  $sv_path
    python3 analysis/ccn_wo_prior_main.py ${params[$i]} "$minTrial" "$in_path"  "$sv_path"
    echo Done 
done
